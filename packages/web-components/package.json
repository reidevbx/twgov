{
  "name": "@gov-tw/web-components",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./twgov-button": {
      "source": "./src/twgov-button.ts",
      "types": "./dist/twgov-button.d.ts",
      "import": "./dist/twgov-button.js"
    },
    "./twgov-checkbox": {
      "source": "./src/twgov-checkbox.ts",
      "types": "./dist/twgov-checkbox.d.ts",
      "import": "./dist/twgov-checkbox.js"
    },
    "./twgov-input": {
      "source": "./src/twgov-input.ts",
      "types": "./dist/twgov-input.d.ts",
      "import": "./dist/twgov-input.js"
    },
    "./twgov-fieldset": {
      "source": "./src/twgov-fieldset.ts",
      "types": "./dist/twgov-fieldset.d.ts",
      "import": "./dist/twgov-fieldset.js"
    },
    "./twgov-stack": {
      "source": "./src/twgov-stack.ts",
      "types": "./dist/twgov-stack.d.ts",
      "import": "./dist/twgov-stack.js"
    },
    "./twgov-sidebar": {
      "source": "./src/twgov-sidebar.ts",
      "types": "./dist/twgov-sidebar.d.ts",
      "import": "./dist/twgov-sidebar.js"
    },
    "./twgov-cluster": {
      "source": "./src/twgov-cluster.ts",
      "types": "./dist/twgov-cluster.d.ts",
      "import": "./dist/twgov-cluster.js"
    },
    "./twgov-container": {
      "source": "./src/twgov-container.ts",
      "types": "./dist/twgov-container.d.ts",
      "import": "./dist/twgov-container.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && vite build --config vite.config.iife.ts",
    "clean": "rm -rf dist",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "vite": "^6.4.0",
    "vite-plugin-dts": "^4.5.0"
  }
}
