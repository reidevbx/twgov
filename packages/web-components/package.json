{
  "name": "@gov-tw/web-components",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./govtw-button": {
      "source": "./src/govtw-button.ts",
      "types": "./dist/govtw-button.d.ts",
      "import": "./dist/govtw-button.js"
    },
    "./govtw-checkbox": {
      "source": "./src/govtw-checkbox.ts",
      "types": "./dist/govtw-checkbox.d.ts",
      "import": "./dist/govtw-checkbox.js"
    },
    "./govtw-input": {
      "source": "./src/govtw-input.ts",
      "types": "./dist/govtw-input.d.ts",
      "import": "./dist/govtw-input.js"
    },
    "./govtw-fieldset": {
      "source": "./src/govtw-fieldset.ts",
      "types": "./dist/govtw-fieldset.d.ts",
      "import": "./dist/govtw-fieldset.js"
    },
    "./govtw-stack": {
      "source": "./src/govtw-stack.ts",
      "types": "./dist/govtw-stack.d.ts",
      "import": "./dist/govtw-stack.js"
    },
    "./govtw-sidebar": {
      "source": "./src/govtw-sidebar.ts",
      "types": "./dist/govtw-sidebar.d.ts",
      "import": "./dist/govtw-sidebar.js"
    },
    "./govtw-cluster": {
      "source": "./src/govtw-cluster.ts",
      "types": "./dist/govtw-cluster.d.ts",
      "import": "./dist/govtw-cluster.js"
    },
    "./govtw-container": {
      "source": "./src/govtw-container.ts",
      "types": "./dist/govtw-container.d.ts",
      "import": "./dist/govtw-container.js"
    },
    "./govtw-textarea": {
      "source": "./src/govtw-textarea.ts",
      "types": "./dist/govtw-textarea.d.ts",
      "import": "./dist/govtw-textarea.js"
    },
    "./govtw-link": {
      "source": "./src/govtw-link.ts",
      "types": "./dist/govtw-link.d.ts",
      "import": "./dist/govtw-link.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && vite build --config vite.config.iife.ts",
    "clean": "rm -rf dist",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "vite": "^6.4.0",
    "vite-plugin-dts": "^4.5.0"
  }
}
